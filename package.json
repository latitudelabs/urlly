{
  "name": "@latitudelabs/urlly",
  "version": "0.3.6",
  "description": "Utility to safe get and update browser url params with ease ðŸ˜‹.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/latitudelabs/urlly.git"
  },
  "bugs": {
    "url": "https://github.com/latitudelabs/urlly/issues"
  },
  "homepage": "https://github.com/latitudelabs/urlly#readme",
  "author": "@latitudelabs/dev-team",
  "license": "ISC",
  "main": "lib/index.js",
  "type": "module",
  "types": "lib/index.d.ts",
  "scripts": {
    "tsc": "tsc",
    "test": "tsd && ava --serial",
    "build": "tsc && tsup",
    "prepare": "husky install",
    "pretty-quick": "pretty-quick --staged"
  },
  "directories": {
    "lib": "lib",
