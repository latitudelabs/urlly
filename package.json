    "get/save/update/parse query params"
  ],
  "sideEffects": false,
  "devDependencies": {
    "ava": "^4.1.0",
    "browser-env": "^3.3.0",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "tsd": "^0.20.0",
    "tsup": "^5.12.4",
    "typescript": "^4.6.3"
  },
  "tsd": {
    "directory": "./tsd"
  },
  "ava": {
    "require": [
      "./test/_browser-env.cjs"
    ]
  }
